{
    "hosting":
    {
        "headers":
        [
            {
                "source": "**/*",
                "headers":
                [
                    {
                        "key": "Content-Security-Policy",
                        "value": "frame-ancestors 'self' http://localhost:4002 https://gopuff.com"
                    },
                    {
                        "key": "X-Content-Type-Options",
                        "value": "nosniff"
                    },
                    {
                        "key": "X-UA-Compatible",
                        "value": "ie=edge"
                    },
                    {
                        "key": "X-XSS-Protection",
                        "value": "1; mode=block"
                    }
                ]
            }
        ],
        "public": "build",
        "ignore":
        [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites":
        [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}